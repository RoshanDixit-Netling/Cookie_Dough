{% comment %} <!-- section -->
<div class="section bg-primary-light">
<div class="container">
<header class="text-center mb-md-5">
<h4>{{ section.settings.heading }}</h4>
</header>
<div class="owl-carousel carousel2">
{% for block in section.blocks %}
{% case block.type %}
{% when 'company' %}
<div class="item">
<img src="{{ block.settings.file-name }}" alt="logo" height="" width="" loading="lazy">
</div>
{% endcase %}
{% endfor %}
</div>
</div>
</div>
<!-- /section --> {% endcomment %}

