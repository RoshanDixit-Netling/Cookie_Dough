      {% comment %}<!-- content -->
      <div id="content">
        {% section 'contact-form' %}
      </div>
      <!-- /content --> {% endcomment %}
      <div class="form">
      {% form 'contact' %}
        <form action="#">
                      <div class="row">
                        <div class="col-md-6 form-group">
                          <input type="text" placeholder="Name" class="form-control" id="{{ formId }}-name" name="contact[name]" value="{% if from[name] %}{{ form[name] }}{% endif %}">
                        </div>
                        <div class="col-md-6 form-group">
                          <input type="text" placeholder="Business Name" class="form-control">
                        </div>
                      </div>
                      <div class="row">
                        <div class="col-md-12 form-group">
                          <input type="tel" placeholder="Phone Number" class="form-control">
                        </div>
                      </div>
                      <div class="row">
                        <div class="col-md-12 form-group">
                          <textarea placeholder="Message" class="form-control"></textarea>
                        </div>
                      </div>
                      <div class="row">
                        <div class="col-md-12 form-group">
                          <button type="button" class="btn btn-primary w-100">{{ section.settings.btn-label }}</button>
                        </div>
                      </div>
                    </form>
      {% endform %}
      </div>